{
  "name": "cspray/database-testing",
  "description": "A framework-agnostic library for setting up a database suitable for automated testing.",
  "type": "library",
  "keywords": [
    "testing",
    "database"
  ],
  "license": ["MIT"],
  "prefer-stable": true,
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phake/phake": "^4.5",
    "phpunit/phpunit": "^11.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\DatabaseTesting\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\DatabaseTesting\\Tests\\": "tests"
    }
  }
}